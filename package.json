    "@mui/material": "5.11.10",
    "@next/bundle-analyzer": "13.2.1",
    "@next/font": "13.2.1",
    "@vercel/analytics": "0.1.10",
    "axios": "1.3.4",
    "color": "^4.2.3",
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "i18next": "22.4.10",
    "lodash": "^4.17.21",
